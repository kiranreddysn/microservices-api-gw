{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/cart/{anything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "cartapi",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/cart/{anything}",
      "UpstreamHttpMethod": [ "GET", "POST" ]
    },
    {
      "DownstreamPathTemplate": "/api/products/{anything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "productsapi",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/products/{anything}",
      "UpstreamHttpMethod": [ "GET", "POST" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://apigw:80"
  }
}
